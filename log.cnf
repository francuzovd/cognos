[loggers]
keys=root,progLogger

[handlers]
keys=consoleHandler,fHandler

[formatters]
keys=progFormatter

[logger_root]
level=ERROR
handlers=consoleHandler

[logger_progLogger]
level=DEBUG
handlers=fHandler
qualname=progLogger
propagate=0

[handler_consoleHandler]
class=StreamHandler
level=INFO
formatter=progFormatter
args=(sys.stdout,)

[handler_fHandler]
class=FileHandler
level=DEBUG
formatter=progFormatter
args=('20201028.log','a')

[formatter_progFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s